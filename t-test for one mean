data<-c(63,63,66,67,68,69,70,70,71,71)
print("enter mu value to be tested")
mu<-scan()
n<-length(data)
print("level of significance")
alpha<-scan()
std<-sd(data)
print(std)
t<-t.test(data,mu=mu)
print(t)
print("table value for two tailed test is:")
tablevalue<-qt(1-alpha/2,df=n-1)
print(round(tablevalue,digits=3))
print("table value for one tailed test is:")
tablevalue<-qt(1-alpha,df=n-1)
print(round(tablevalue,digits=3))
